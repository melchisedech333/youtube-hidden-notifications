{
    "name": "YouTube Hidden Notifications!",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "YouTube Hidden Notifications",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "background.js"
            ]
        }
    ],
    "permissions": [
        "tabs", 
        "https://*/*"
    ]
}


